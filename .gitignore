# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore storage
/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key
/config/credentials/*.key

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore node_modules
/node_modules

# Ignore vendor bundle
/vendor/bundle

# Ignore assets precompile
/public/assets

# Mac files
.DS_Store
